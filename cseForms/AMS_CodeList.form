<?xml version="1.0" encoding="utf-8"?>
<?data-format version="0.400"?>
<Form>
  <Name value="AMS_CodeList" />
  <Content>
    <Panel>
      <Name value="AMS_CodeListPnl" />
      <Orientation value="Vertical" />
      <Panel.Height value="#" />
      <Panel.Width value="*" />
      <Children>
        <Panel>
          <Name value="pnl_Body" />
          <Orientation value="Vertical" />
          <Panel.Height value="#" />
          <Panel.Width value="50%" />
          <Children>
            <Panel>
              <Name value="pnl_Breadcrumbs" />
              <Orientation value="Horizontal" />
              <Panel.Height value="#" />
              <Panel.Width value="*" />
              <CssClass>
                <CssClassConditional>
                  <ClassName value="{theme}-panel" />
                  <IsThemedStyle value="True" />
                </CssClassConditional>
                <CssClassConditional>
                  <ClassName value="{theme}-breadcrumbs" />
                  <IsThemedStyle value="True" />
                </CssClassConditional>
              </CssClass>
            </Panel>
            <Panel>
              <Name value="edb_CodeList_parent" />
              <Orientation value="Horizontal" />
              <Panel.Height value="#" />
              <Panel.Width value="*" />
              <Children>
                <TextBox>
                  <Name value="edb_CodeList" />
                  <CaptionPosition value="Top" />
                  <IsEnabled value="False" />
                  <Panel.Height value="#" />
                  <Panel.Width value="50%" />
                  <Bindings>
                    <Binding SourceProperty="Data" Expression="" />
                    <Binding SourceProperty="Caption" Expression="localized.NazevCiselniku" />
                  </Bindings>
                  <CssClass>
                    <CssClassConditional>
                      <ClassName value="{theme}-textbox" />
                      <IsThemedStyle value="True" />
                    </CssClassConditional>
                  </CssClass>
                </TextBox>
                <DatePicker>
                  <Name value="dtp_CodeListDate" />
                  <CaptionPosition value="Top" />
                  <IsEnabled value="False" />
                  <Panel.Height value="#" />
                  <Panel.Width value="50%" />
                  <Bindings>
                    <Binding SourceProperty="Data" Expression="" />
                    <Binding SourceProperty="Caption" Expression="localized.DataCiselnikuPlatneKDatu" />
                  </Bindings>
                  <CssClass>
                    <CssClassConditional>
                      <ClassName value="{theme}-datepicker" />
                      <IsThemedStyle value="True" />
                    </CssClassConditional>
                  </CssClass>
                </DatePicker>
              </Children>
              <CssClass>
                <CssClassConditional>
                  <ClassName value="{theme}-panel" />
                  <IsThemedStyle value="True" />
                </CssClassConditional>
              </CssClass>
            </Panel>
            <DataTable>
              <Name value="tab_CodeList" />
              <Panel.Height value="#" />
              <Panel.Width value="*" />
              <Bindings>
                <Binding SourceProperty="DataItems" Expression="" />
              </Bindings>
              <Columns>
                <DataColumn>
                  <Name value="edb_CodeCol" />
                  <Content>
                    <TextBox>
                      <Name value="edb_Code" />
                      <CaptionPosition value="Hide" />
                      <IsEnabled value="False" />
                      <Bindings>
                        <Binding SourceProperty="Data" Expression="" />
                        <Binding SourceProperty="Caption" Expression="localized.LocalizedString2" />
                      </Bindings>
                      <CssClass>
                        <CssClassConditional>
                          <ClassName value="{theme}-textbox" />
                          <IsThemedStyle value="True" />
                        </CssClassConditional>
                      </CssClass>
                    </TextBox>
                  </Content>
                  <Bindings>
                    <Binding SourceProperty="Caption" Expression="localized.Kod" />
                  </Bindings>
                  <HeaderCssClass>
                    <CssClassConditional>
                      <ClassName value="{theme}-datacolumn" />
                      <IsThemedStyle value="True" />
                    </CssClassConditional>
                  </HeaderCssClass>
                </DataColumn>
                <DataColumn>
                  <Name value="edb_NameCol" />
                  <Content>
                    <Panel>
                      <Name value="panel2" />
                      <Orientation value="Horizontal" />
                      <Children>
                        <TextBox>
                          <Name value="edb_Name" />
                          <CaptionPosition value="Hide" />
                          <IsEnabled value="False" />
                          <Panel.Height value="#" />
                          <Panel.Width value="*" />
                          <Bindings>
                            <Binding SourceProperty="Data" Expression="" />
                            <Binding SourceProperty="Caption" Expression="localized.LocalizedString2" />
                          </Bindings>
                          <CssClass>
                            <CssClassConditional>
                              <ClassName value="{theme}-textbox" />
                              <IsThemedStyle value="True" />
                            </CssClassConditional>
                          </CssClass>
                        </TextBox>
                      </Children>
                      <CssClass>
                        <CssClassConditional>
                          <ClassName value="{theme}-panel" />
                          <IsThemedStyle value="True" />
                        </CssClassConditional>
                      </CssClass>
                    </Panel>
                  </Content>
                  <Bindings>
                    <Binding SourceProperty="Caption" Expression="localized.Nazev" />
                  </Bindings>
                  <HeaderCssClass>
                    <CssClassConditional>
                      <ClassName value="{theme}-datacolumn" />
                      <IsThemedStyle value="True" />
                    </CssClassConditional>
                  </HeaderCssClass>
                </DataColumn>
              </Columns>
              <CssClass>
                <CssClassConditional>
                  <ClassName value="{theme}-datatable" />
                  <IsThemedStyle value="True" />
                </CssClassConditional>
              </CssClass>
            </DataTable>
          </Children>
          <CssClass>
            <CssClassConditional>
              <ClassName value="{theme}-panel" />
              <IsThemedStyle value="True" />
            </CssClassConditional>
          </CssClass>
        </Panel>
      </Children>
      <CssClass>
        <CssClassConditional>
          <ClassName value="{theme}-panel" />
          <IsThemedStyle value="True" />
        </CssClassConditional>
      </CssClass>
    </Panel>
  </Content>
  <DataContext>
    <DataContext>
      <Children>
        <Complex>
          <Name value="model" />
          <Caption value="Model" />
          <PropertyLocalizationData.PropertyLocalizations>
            <PropertyLocalizationData>
              <LocalizedProperties>
                <LocalizedProperty>
                  <Localization reference="+Localizations.Model" />
                  <PropertyName value="Caption" />
                </LocalizedProperty>
              </LocalizedProperties>
            </PropertyLocalizationData>
          </PropertyLocalizationData.PropertyLocalizations>
        </Complex>
      </Children>
    </DataContext>
  </DataContext>
  <OnCreate>
    <ActionEvent ReferencedAction="~handler_AMS_CodeList_OnCreate" />
  </OnCreate>
  <Actions>
    <ActionBlock>
      <Name value="setBreadcrumbs" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//apiobjects.breadcrumbsCtrl.init(globals.attribute1)
//apiobjects.breadcrumbsCtrl.removeAll()
//apiobjects.breadcrumbsCtrl.addBreadcrumb("amsCodelist", "AMSCodeList", defaultString(), defaultString(), defaultBool())
system.console("oK")]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_AMS_CodeList_OnCreate" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var url = packages.main.apiobjects.urlRouter.findFormUrl(navigation.cseForms.AMS_CodeList.fullName)
url.updatePageUrl()

form.actions.setBreadcrumbs()]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
  </Actions>
  <Bindings>
    <Binding SourceProperty="Caption" Expression="localized.ZobrazeniDatCiselniku" />
  </Bindings>
</Form>